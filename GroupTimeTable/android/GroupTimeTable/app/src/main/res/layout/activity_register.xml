<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#ffffff">

    <dreamingworm.grouptimetable.CustomViews.CustomTitleView
        android:layout_width="wrap_content"
        android:layout_height="60dp"
        android:text="회원가입"
        android:layout_gravity="center_horizontal"
        android:textSize="60dp"
        android:layout_marginTop="40dp"
        android:layout_marginBottom="40dp" />

    <LinearLayout
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <LinearLayout
            android:orientation="vertical"
            android:layout_width="360dp"
            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal"
            android:layout_marginBottom="10dp">

            <LinearLayout
                android:layout_width="280dp"
                android:layout_height="wrap_content"
                android:layout_marginLeft="40dp"
                android:layout_marginRight="40dp"
                android:orientation="horizontal">
                <dreamingworm.grouptimetable.CustomViews.CustomContentTextView
                    android:layout_width="60dp"
                    android:layout_height="40dp"
                    android:text="아이디"
                    android:gravity="right"
                    android:layout_marginRight="20dp"
                    android:textSize="15dp" />
                <dreamingworm.grouptimetable.CustomViews.CustomEditText1
                    android:id="@+id/register_ID_Edt"
                    android:layout_width="200dp"
                    android:layout_height="40dp"
                    android:textSize="15dp"
                    android:maxLines="1"
                    android:maxLength="20"
                    android:inputType="text" />
            </LinearLayout>

            <dreamingworm.grouptimetable.CustomViews.CustomContentTextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/register_ID_Txt"
                android:layout_marginLeft="120dp"
                android:visibility="gone"
                android:textAlignment="viewEnd"
                android:textSize="10dp" />

        </LinearLayout>

        <LinearLayout
            android:orientation="vertical"
            android:layout_width="360dp"
            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal"
            android:layout_marginBottom="10dp">

            <LinearLayout
                android:layout_width="280dp"
                android:layout_height="wrap_content"
                android:layout_marginLeft="40dp"
                android:layout_marginRight="40dp"
                android:orientation="horizontal">

                <dreamingworm.grouptimetable.CustomViews.CustomContentTextView
                    android:layout_width="60dp"
                    android:layout_height="40dp"
                    android:text="암호"
                    android:gravity="right"
                    android:layout_marginRight="20dp"
                    android:textSize="15dp" />
                <dreamingworm.grouptimetable.CustomViews.CustomEditText1
                    android:id="@+id/register_Password_Edt"
                    android:layout_width="200dp"
                    android:layout_height="40dp"
                    android:inputType="textPassword|text"
                    android:textSize="15dp"
                    android:maxLines="1"
                    android:maxLength="20" />

            </LinearLayout>

            <dreamingworm.grouptimetable.CustomViews.CustomContentTextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/register_Password_Txt"
                android:layout_marginLeft="120dp"
                android:visibility="gone"
                android:textAlignment="viewEnd"
                android:textSize="10dp" />

        </LinearLayout>

        <LinearLayout
            android:orientation="vertical"
            android:layout_width="360dp"
            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal"
            android:layout_marginBottom="10dp">

            <LinearLayout
                android:layout_width="280dp"
                android:layout_height="wrap_content"
                android:layout_marginLeft="40dp"
                android:layout_marginRight="40dp"
                android:orientation="horizontal">
                <dreamingworm.grouptimetable.CustomViews.CustomContentTextView
                    android:layout_width="60dp"
                    android:layout_height="40dp"
                    android:text="암호확인"
                    android:gravity="right"
                    android:layout_marginRight="20dp"
                    android:textSize="15dp" />
                <dreamingworm.grouptimetable.CustomViews.CustomEditText1
                    android:id="@+id/register_PasswordChk_Edt"
                    android:layout_width="200dp"
                    android:layout_height="40dp"
                    android:inputType="textPassword"
                    android:textSize="15dp"
                    android:maxLines="1"
                    android:maxLength="20" />
            </LinearLayout>

            <dreamingworm.grouptimetable.CustomViews.CustomContentTextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/register_PasswordChk_Txt"
                android:layout_marginLeft="120dp"
                android:visibility="gone"
                android:textAlignment="viewEnd"
                android:textSize="10dp" />
        </LinearLayout>

        <LinearLayout
            android:orientation="vertical"
            android:layout_width="360dp"
            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal"
            android:layout_marginBottom="10dp">

            <LinearLayout
                android:layout_width="280dp"
                android:layout_height="wrap_content"
                android:layout_marginLeft="40dp"
                android:layout_marginRight="40dp"
                android:orientation="horizontal">
                <dreamingworm.grouptimetable.CustomViews.CustomContentTextView
                    android:text="휴대번호"
                    android:layout_width="60dp"
                    android:layout_height="40dp"
                    android:gravity="right"
                    android:layout_marginRight="20dp"
                    android:textSize="15dp" />
                <dreamingworm.grouptimetable.CustomViews.CustomEditText1
                    android:id="@+id/register_Phone_Edt"
                    android:layout_width="200dp"
                    android:layout_height="40dp"
                    android:inputType="phone"
                    android:textSize="15dp"
                    android:maxLines="1"
                    android:maxLength="11" />
            </LinearLayout>

            <dreamingworm.grouptimetable.CustomViews.CustomContentTextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/register_Phone_Txt"
                android:layout_marginLeft="120dp"
                android:visibility="gone"
                android:textAlignment="viewEnd"
                android:textSize="10dp" />
        </LinearLayout>

        <LinearLayout
            android:orientation="vertical"
            android:layout_width="360dp"
            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal"
            android:layout_marginBottom="10dp">

            <LinearLayout
                android:layout_width="280dp"
                android:layout_height="wrap_content"
                android:layout_marginLeft="40dp"
                android:layout_marginRight="40dp"
                android:orientation="horizontal">
                <dreamingworm.grouptimetable.CustomViews.CustomContentTextView
                    android:layout_width="60dp"
                    android:layout_height="40dp"
                    android:text="이름"
                    android:gravity="right"
                    android:layout_marginRight="20dp"
                    android:textSize="15dp" />
                <dreamingworm.grouptimetable.CustomViews.CustomEditText1
                    android:id="@+id/register_Name_Edt"
                    android:layout_width="200dp"
                    android:layout_height="40dp"
                    android:textSize="15dp"
                    android:maxLines="1"
                    android:maxLength="5"
                    android:inputType="text" />
            </LinearLayout>

            <dreamingworm.grouptimetable.CustomViews.CustomContentTextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/register_Name_Txt"
                android:layout_marginLeft="120dp"
                android:visibility="gone"
                android:textAlignment="viewEnd"
                android:textSize="10dp" />
        </LinearLayout>

        <LinearLayout
            android:orientation="vertical"
            android:layout_width="360dp"
            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal"
            android:layout_marginBottom="10dp">

            <LinearLayout
                android:layout_width="280dp"
                android:layout_height="wrap_content"
                android:layout_marginLeft="40dp"
                android:layout_marginRight="40dp"
                android:orientation="horizontal">
                <dreamingworm.grouptimetable.CustomViews.CustomContentTextView
                    android:layout_width="60dp"
                    android:layout_height="40dp"
                    android:text="닉네임"
                    android:gravity="right"
                    android:layout_marginRight="20dp"
                    android:textSize="15dp" />
                <dreamingworm.grouptimetable.CustomViews.CustomEditText1
                    android:id="@+id/register_Nickname_Edt"
                    android:layout_width="200dp"
                    android:layout_height="40dp"
                    android:textSize="15dp"
                    android:maxLines="1"
                    android:maxLength="6"
                    android:inputType="text" />
            </LinearLayout>

            <dreamingworm.grouptimetable.CustomViews.CustomContentTextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/register_Nickname_Txt"
                android:layout_marginLeft="120dp"
                android:visibility="gone"
                android:textAlignment="viewEnd"
                android:textSize="10dp" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_marginTop="40dp">
            <dreamingworm.grouptimetable.CustomViews.CustomButtonOrange
                android:id="@+id/register_Register_Btn"
                android:layout_width="120dp"
                android:layout_height="50dp"
                android:text="등록"
                android:layout_gravity="center_horizontal"
                android:layout_marginRight="40dp"
                android:layout_marginLeft="40dp"
                android:textSize="23dp" />
            <dreamingworm.grouptimetable.CustomViews.CustomButtonOrange
                android:id="@+id/register_Cancel_Btn"
                android:layout_width="120dp"
                android:layout_height="50dp"
                android:text="취소"
                android:layout_marginRight="40dp"
                android:textSize="23dp" />
        </LinearLayout>
    </LinearLayout>

</LinearLayout>